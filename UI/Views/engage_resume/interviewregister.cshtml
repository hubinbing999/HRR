<script src="~/javascript/vue.js"></script>
<script type="text/javascript">
    var vm = new Vue({
        el: "#div123",
        data: {
            row1: null
        }, methods: {
            load:function(){
                $.ajax({
                    url: "/engage_resume/interviewregister2",
                    type: "post",
                    data: $("#form").serialize(),
                    success: function (msg) {
                        if (msg > 0) {
                            var s = Number(msg)+1;
                            $("#mscs").append(" <input type='text' value='第" + s + "次面试' class='INPUT_STYLE2' readonly='readonly' name='interview_amount' />");
                        } else {
                            $("#mscs").append(" <input type='text' value='第1次面试' class='INPUT_STYLE2' readonly='readonly' name='interview_amount' />");
                        }
                    }
                })
            },
            fh: function () {
                window.location.href = "/engage_resume/interviewlist";
            },
            msdj: function () {
                var image_degree = $("#image_degree").find("option:selected").val();
                var native_language_degree = $("#native_language_degree").find("option:selected").val();
                var foreign_language_degree = $("#foreign_language_degree").find("option:selected").val();
                var response_speed_degree = $("#response_speed_degree").find("option:selected").val();
                var EQ_degree = $("#EQ_degree").find("option:selected").val();
                var IQ_degree = $("#IQ_degree").find("option:selected").val();
                var multi_quality_degree = $("#multi_quality_degree").find("option:selected").val();
                var register = $("#register").find("option:selected").val();
                if ($("#date").val() == "") {
                    alert("面试时间不能为空");
                    return false;
                }
                if ($("#interview_comment").val() == "") {
                    alert("面试评价不能为空");
                    return false;
                }
                $.ajax({
                    url: "/engage_resume/interviewregister",
                    type: "post",
                    data: $("#form").serialize() + "&image_degree1=" + image_degree + "&native_language_degree1=" + native_language_degree + "&foreign_language_degree1=" + foreign_language_degree + "&response_speed_degree1=" + response_speed_degree
                    + "&EQ_degree1=" + EQ_degree + "&IQ_degree1=" + IQ_degree + "&multi_quality_degree1=" + multi_quality_degree + "&register1=" + register,
                    success: function (msg) {
                        if (msg > 0) {
                            alert("登记成功");
                            window.location.href = "/engage_resume/interviewlist";
                        } else {
                            alert("登记失败");
                            window.location.href = "/engage_resume/interviewlist";
                        }
                    }
                })
                    
            }
        }, created: function () {
            this.load();
        }
    })
</script>