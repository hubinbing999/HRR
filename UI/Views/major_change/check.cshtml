
@{
    Layout = null;
}
@model Model.major_changeModel
<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>check</title>
    <link href="~/css/table.css" rel="stylesheet" />
    <script src="~/Scripts/jquery-1.10.2.js"></script>
    <script src="~/Scripts/vue.js"></script>
    <script type="text/javascript">
        $(function () {
            var hidRadio = $("#hidSex").val();
            var radios = document.getElementsByName("check_status");
            if (hidRadio == 1) {
                radios[0].checked = true;
            } else {
                radios[1].checked = true;
            }
        })
        $(function () {
            $("#Button1").click(function () {
              //  alert($("input[name='check_status']:checked").val());
            })
            $("#But").click(function () {
                window.location.href = '/major_change/check_list';
            })
            $(function () {
                $("#fhr").val(localStorage.getItem("a"));
            })
        })
    </script>
</head>
<body>
    
@using (Ajax.BeginForm("check", new AjaxOptions() { HttpMethod = "post" }))
{

        <table width="100%">
            <tr>
                <td>
                    <font color="#0000CC">您正在做的业务是:人力资源--调动管理--调动登记</font>
                </td>
            </tr>

            <tr>
                <td align="right">
                    <input type="hidden" id="hidSex" value="@Model.check_status" />
                    <input type="radio" name="check_status" value="通过" />通过<input type="radio" name="check_status" value="不通过" />不通过
                    <input id="Button1" type="submit" value="提交" class="BUTTON_STYLE1" />
                    <input id="But" type="button" value="返回" class="BUTTON_STYLE1" />
                </td>
            </tr>
        </table>
        <table width="100%" border="1" cellpadding=0 cellspacing=1
               bordercolorlight=#848284 bordercolordark=#eeeeee
               class="TABLE_STYLE1">
            <tr height="21">
                <td class="TD_STYLE1" width="8%">
                    档案编号
                </td>
                <td class="TD_STYLE2" width="10%">
                    @Html.TextBox("mch_id", Model.mch_id, new { @class = "INPUT_STYLE2", @readonly = "readonly", @hidden = "hidden", @id = "djr" })
                    @Html.TextBox("human_id", Model.human_id, new { @class = "INPUT_STYLE2", @readonly = "readonly" })

                </td>
                <td class="TD_STYLE1" width="8%">
                    姓名
                </td>
                <td class="TD_STYLE2" width="10%">
                    @Html.TextBox("human_name", Model.human_name, new { @class = "INPUT_STYLE2", @readonly = "readonly" })
                </td>
                <td class="TD_STYLE1" width="8%"></td>
                <td class="TD_STYLE2" width="10%"></td>
                <td class="TD_STYLE1" width="8%"></td>
                <td class="TD_STYLE2" width="10%"></td>
            </tr>
            <tr>

                <td class="TD_STYLE1" width="8%">
                    原一级机构
                </td>
                <td class="TD_STYLE2">
                    @Html.TextBox("first_kind_id", Model.first_kind_id, new { @class = "INPUT_STYLE2", @style = "display:none" })
                    @Html.TextBox("first_kind_name", Model.first_kind_name, new { @class = "INPUT_STYLE2", @readonly = "readonly" })
                </td>
                <td class="TD_STYLE1" width="8%">
                    原二级机构
                </td>
                <td class="TD_STYLE2" width="10%">
                    @Html.TextBox("second_kind_id", Model.second_kind_id, new { @class = "INPUT_STYLE2", @style = "display:none" })
                    @Html.TextBox("second_kind_name", Model.second_kind_name, new { @class = "INPUT_STYLE2", @readonly = "readonly" })
                </td>
                <td class="TD_STYLE1" width="8%">
                    原三级机构
                </td>
                <td class="TD_STYLE2" width="10%">
                    @Html.TextBox("third_kind_id", Model.third_kind_id, new { @class = "INPUT_STYLE2", @style = "display:none" })
                    @Html.TextBox("third_kind_name", Model.third_kind_name, new { @class = "INPUT_STYLE2", @readonly = "readonly" })
                </td>
                <td class="TD_STYLE1" width="8%"></td>
                <td class="TD_STYLE2" width="10%"></td>
            </tr>

            <tr>
                <td class="TD_STYLE1">
                    原职位分类
                </td>
                <td class="TD_STYLE2">
                    @Html.TextBox("major_kind_id", Model.major_kind_id, new { @class = "INPUT_STYLE2", @style = "display:none" })
                    @Html.TextBox("major_kind_name", Model.major_kind_name, new { @class = "INPUT_STYLE2", @readonly = "readonly" })
                </td>
                <td class="TD_STYLE1" width="12%">
                    原职位名称
                </td>
                <td class="TD_STYLE2">
                    @Html.TextBox("major_id", Model.major_id, new { @class = "INPUT_STYLE2", @style = "display:none" })
                    @Html.TextBox("major_name", Model.major_name, new { @class = "INPUT_STYLE2", @readonly = "readonly" })
                </td>
                <td class="TD_STYLE1">
                    原薪酬标准
                </td>
                <td class="TD_STYLE2">
                    @Html.TextBox("salary_standard_id", Model.salary_standard_id, new { @class = "INPUT_STYLE2", @style = "display:none" })
                    @Html.TextBox("salary_standard_name", Model.salary_standard_name, "", new { @class = "INPUT_STYLE2", @readonly = "readonly" })
                </td>
                <td class="TD_STYLE1">
                    原薪酬总额
                </td>
                <td class="TD_STYLE2">
                    @Html.TextBox("salary_sum", Model.salary_sum, new { @class = "INPUT_STYLE2", @readonly = "readonly" })
                </td>
            </tr>
            <tr>
                <td class="TD_STYLE1">
                    新一级机构名称
                </td>
                <td class="TD_STYLE2">
                    @Html.TextBox("new_first_kind_id", Model.new_first_kind_id, "", new { @class = "INPUT_STYLE2", @style = "display:none" })
                    @Html.TextBox("new_first_kind_name", Model.new_first_kind_name, "", new { @class = "INPUT_STYLE2", @readonly = "readonly" })
                </td>
                <td class="TD_STYLE1" width="12%">
                    新二级机构名称
                </td>
                <td class="TD_STYLE2">
                    @Html.TextBox("new_second_kind_id", Model.new_second_kind_id, "", new { @class = "INPUT_STYLE2", @style = "display:none" })
                    @Html.TextBox("new_second_kind_name", Model.new_second_kind_name, "", new { @class = "INPUT_STYLE2", @readonly = "readonly" })
                </td>
                <td class="TD_STYLE1" width="12%">
                    新三级机构名称
                </td>
                <td class="TD_STYLE2">
                    @Html.TextBox("new_third_kind_id", Model.new_third_kind_id, "", new { @class = "INPUT_STYLE2", @style = "display:none" })
                    @Html.TextBox("new_third_kind_name", Model.new_third_kind_name, "", new { @class = "INPUT_STYLE2", @readonly = "readonly" })
                </td>
                <td class="TD_STYLE1" width="8%"></td>
                <td class="TD_STYLE2" width="10%"></td>
            </tr>
            <tr>
                <td class="TD_STYLE1">
                    新职位分类
                </td>
                <td class="TD_STYLE2">
                    @Html.TextBox("new_major_kind_id", Model.new_major_kind_id, "", new { @class = "INPUT_STYLE2", @style = "display:none" })
                    @Html.TextBox("new_major_kind_name", Model.new_major_kind_name, "", new { @class = "INPUT_STYLE2", @readonly = "readonly" })
                </td>
                <td class="TD_STYLE1" width="12%">
                    新职位名称
                </td>
                <td class="TD_STYLE2">
                    @Html.TextBox("new_major_id", Model.new_major_id, "", new { @class = "INPUT_STYLE2", @style = "display:none" })
                    @Html.TextBox("new_major_name", Model.new_major_name, "", new { @class = "INPUT_STYLE2", @readonly = "readonly" })
                </td>
                <td class="TD_STYLE1">
                    新薪酬标准
                </td>
                <td class="TD_STYLE2">
                    @Html.TextBox("new_salary_standard_id", Model.new_salary_standard_id, "", new { @class = "INPUT_STYLE2", @style = "display:none" })
                    @Html.TextBox("new_salary_standard_name", Model.new_salary_standard_name, "", new { @class = "INPUT_STYLE2", @readonly = "readonly" })
                </td>
                <td class="TD_STYLE1">
                    新薪酬总额
                </td>
                <td class="TD_STYLE2">
                    @Html.TextBox("new_salary_sum", Model.new_salary_sum, "", new { @class = "INPUT_STYLE2", @readonly = "readonly" })
                </td>
            </tr>

            <tr>
                <td class="TD_STYLE1">
                    登记人
                </td>
                <td class="TD_STYLE2">
                    @Html.TextBox("register", Model.register, new { @class = "INPUT_STYLE2", @readonly = "readonly" })
                </td>
                <td class="TD_STYLE1">
                    登记时间
                </td>
                <td class="TD_STYLE2">
                    @Html.TextBox("regist_time", Model.regist_time, new { @class = "INPUT_STYLE2", @readonly = "readonly" })
                </td>
                <td class="TD_STYLE1">
                    复核人
                </td>
                <td class="TD_STYLE2">
                    @Html.TextBox("checker", Model.checker, new { @class = "INPUT_STYLE2", @readonly = "readonly", @id = "fhr" })
                </td>
                <td class="TD_STYLE1">
                    复核时间
                </td>
                <td class="TD_STYLE2">
                    <input type="text" name="check_time" readonly="readonly"
                           id="Tdate" class="INPUT_STYLE2">
                    </td>
                <script type="text/javascript">
					function time(){
 					var tdate=document.getElementById("Tdate");
 					var d=new Date();
 					var y=d.getFullYear();
 					var moth=d.getMonth()+1;
 					var date=d.getDate();
 					var h=d.getHours();
 					var m=d.getMinutes();
 					var s=d.getSeconds();
					var hh=20-1-h;
					var mm=60-1-m;
					var ss=60-s;
					if(hh>0){
							mm=mm+60*hh;
					}
 					if(moth<10){
 							moth="0"+moth;
 					}
 					if(date<10){
 							date="0"+date;
 					}
 					if(h<10){
 							h="0"+h;
 					}
 					if(m<10){
 							m="0"+m;
 					}
 					if(s<10){
 							s="0"+s;
 					}
 					if(mm<10){
 							mm="0"+mm;
 					}
 					if(ss<10){
 							ss="0"+ss;
 					}

 					tdate.value=y+"-"+moth+"-"+date+" "+h+":"+m+":"+s;


 				}
				window.onload=time;
 				var out=window.setInterval(time, 1000);
                </script>
            </tr>
            <tr>
                <td class="TD_STYLE1">
                    调动原因
                </td>
                <td colspan="7" class="TD_STYLE2">
                    @Html.TextArea("change_reason", Model.change_reason, new { @class = "TEXTAREA_STYLE1" })
                </td>
            </tr>
            <tr>
                <td class="TD_STYLE1">
                    复合通过意见
                </td>
                <td colspan="7" class="TD_STYLE2">
                    @Html.TextArea("check_reason", Model.check_reason, new { @class = "TEXTAREA_STYLE1" })
                </td>
            </tr>
        </table>
    }
</body>
</html>
